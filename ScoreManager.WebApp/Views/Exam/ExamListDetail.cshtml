
@using Models
@model List<EDU_EXAM>

<div class="layui-card-body ">
    <table class="layui-table layui-form">
        <thead>
            <tr>

                <th>ID</th>
                <th>考试名称</th>
                <th>班级</th>
                <th>学科</th>
                <th>教务老师</th>
                <th>阅卷老师</th>
                <th>开始时间</th>
                <th>结束时间</th>
                <th>是否发布</th>
                <th>操作</th>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                    <td>@item.ID</td>
                    <td>@item.NAME</td>
                    <td>@item.Exam_Class?.NAME</td>
                    <td>@item.Exam_Subject?.NAME</td>
                    <td>@item.Exam_EducationTeacher?.NAME</td>
                    <td>@item.Exam_MarkPaperTeacher?.NAME</td>
                    <td>@item.STARTTIME</td>
                    <td>@item.ENDTIME</td>
                    <td>@(item.ISPUB=="0"?"未发布":"已发布")</td>
                    <td class="td-manage">
                        <a title="发布" onclick="ReleaseExam(@item.ID)" href="javascript:;">
                            <i class="layui-icon">&#xe609;</i>
                        </a>
                        <a title="删除" onclick="member_del(this,@item.ID)" href="javascript:;">
                            <i class="layui-icon">&#xe640;</i>
                        </a>
                    </td>
                </tr>
            }

        </tbody>
    </table>
</div>



