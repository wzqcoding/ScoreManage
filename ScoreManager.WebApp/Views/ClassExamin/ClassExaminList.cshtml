
@using Models
@model List<EDU_EXAM>

<div class="layui-card-body ">
    <table class="layui-table layui-form">
        <thead>
            <tr>

                <th>ID</th>
                <th>考试名称</th>
                <th>班级</th>
                <th>学科</th>
                <th>教务老师</th>
                <th>阅卷老师</th>
                <th>开始时间</th>
                <th>结束时间</th>
                <th>操作</th>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                    <td>@item.ID</td>
                    <td>@item.NAME</td>
                    <td>@item.Exam_Class?.NAME</td>
                    <td>@item.Exam_Subject?.NAME</td>
                    <td>@item.Exam_EducationTeacher?.NAME</td>
                    <td>@item.Exam_MarkPaperTeacher?.NAME</td>
                    <td>@item.STARTTIME</td>
                    <td>@item.ENDTIME</td>
                    <td class="td-manage">
                        <button type="button" onclick="xadmin.open('班级成绩详情','/ClassExamin/ClassExaminDetail?examId='+@item.ID,600,600)" class="layui-btn">查看</button>
                    </td>
                </tr>
            }

        </tbody>
    </table>
</div>



